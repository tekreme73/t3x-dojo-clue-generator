{
  "shell": "src/t3x-dojo-clue-generator-app.js",
  "entrypoint": "index.html",
  "npm": true,
  "moduleResolution": "node",
  "sources": [
    "src/**.js",
    "helpers.js",
    "package.json"
  ],
  "extraDependencies": [
    "node_modules/@webcomponents/webcomponentsjs/**",
    ".gitignore",
    "LICENSE"
  ],
  "builds": [
    {
      "bundle": true,
      "basePath": "/t3x-dojo-clue-generator/",
      "js": {
        "minify": false,
        "compile": "es5",
        "transformModulesToAmd": true
      }
    }
  ],
  "lint": {
    "rules": [
      "polymer-3"
    ]
  }
}